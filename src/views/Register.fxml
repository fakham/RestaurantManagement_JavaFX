    <children>
        <Label prefHeight="600.0" prefWidth="530.0" style="-fx-background-color: #b71c1c;" textFill="#460101" />
        <ImageView fitHeight="241.0" fitWidth="241.0" layoutX="103.0" layoutY="135.0" pickOnBounds="true" preserveRatio="true" x="5.0" y="5.0">
            <image>
                <Image url="@../images/logo.png" />
            </image>
        </ImageView>
        <TextField fx:id="txtFirstName" layoutX="646.0" layoutY="107.0" prefHeight="33.0" prefWidth="274.0" promptText="First Name" />
        <TextField fx:id="txtLastName" layoutX="646.0" layoutY="169.0" prefHeight="33.0" prefWidth="274.0" promptText="Last Name" />
        <TextField fx:id="txtUsername" layoutX="646.0" layoutY="234.0" prefHeight="33.0" prefWidth="274.0" promptText="UserName" />
        <TextField fx:id="txtEmail" layoutX="646.0" layoutY="307.0" prefHeight="33.0" prefWidth="274.0" promptText="Email" />
